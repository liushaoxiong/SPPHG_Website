DIV.scott {
PADDING-BOTTOM: 3px; PADDING-TOP: 3px; TEXT-ALIGN: right
}
DIV.scott A {
 PADDING-RIGHT: 5px; PADDING-LEFT: 5px; PADDING-BOTTOM: 2px; BORDER: #9a252e 1px solid; COLOR: #9a252e; MARGIN-RIGHT: 2px; PADDING-TOP: 2px;TEXT-DECORATION: none
}
DIV.scott A:hover {
BORDER: #9a252e 1px solid; BACKGROUND-COLOR: #9a252e; color:#FFFFFF
}
DIV.scott SPAN.current {
 BORDER: #9a252e 1px solid; PADDING: 2px 5px 2px 5px; FONT-WEIGHT: bold; COLOR: #fff;BACKGROUND-COLOR: #9a252e; margin:0px;
}
DIV.scott SPAN.disabled {
 BORDER: #9a252e 1px solid;PADDING-BOTTOM: 2px; COLOR: #9a252e; MARGIN-RIGHT: 2px; PADDING-TOP: 2px; 
}

